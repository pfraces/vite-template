{
  "name": "vite-template",
  "version": "0.1.0",
  "description": "Template for Vite projects",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "format": "prettier --write . !root",
    "lint:md": "markdownlint-cli2 **/*.md !node_modules !root",
    "deps:check": "ncu",
    "deps:update": "ncu --upgrade && npm install"
  },
  "devDependencies": {
    "markdownlint-cli2": "^0.13.0",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.3.3"
  }
}
